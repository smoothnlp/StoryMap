{
    "manifest_version": 2,
    "name": "StoryMap",
    "description": "Opens mymap.ai with current URL",
    "version": "1.0",
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "commands": {
      "_execute_browser_action": {
        "suggested_key": {
          "default": "Ctrl+Shift+M",
          "mac": "MacCtrl+Shift+M"
        }
      }
    },
    "permissions": [
      "activeTab"
    ],
    "browser_action": {}
  }
  